   "source": [
    "import numpy as np\n",
    "\n",
    "N = 1000000\n",
    "M = 3\n",
    "heads = np.zeros(M+1)\n",
    "for i in range(N):\n",
    "    flips = np.random.randint(0, 2, M)\n",
    "    h, _ = np.bincount(flips, minlength=2)\n",
    "    heads[h] += 1\n",
    "prob = heads / N\n",
    "print(\"Probabilities: \", np.array2string(prob))"
   ]
